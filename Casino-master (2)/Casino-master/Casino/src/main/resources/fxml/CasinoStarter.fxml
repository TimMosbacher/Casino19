            <Font size="24.0" />
         </font>
      </Button>
      <Button fx:id="slotMach" layoutX="1000.0" layoutY="500.0" mnemonicParsing="false" onAction="#slotMach" prefHeight="100.0" prefWidth="250.0" text="Slot Machine">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button fx:id="roulette" layoutX="1000.0" layoutY="650.0" mnemonicParsing="false" onAction="#roulette" prefHeight="100.0" prefWidth="250.0" text="Roulette">
