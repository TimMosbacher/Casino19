<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="800.0" prefWidth="1300.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/10.0.1" fx:controller="com.mycompany.casino.CasinoStarterController">
    <stylesheets>
        <URL value="@/styles/casinostarter.css" />
    </stylesheets>
   <children>
      <Button fx:id="bingo" layoutX="1000.0" layoutY="200.0" mnemonicParsing="false" onAction="#bingo" prefHeight="100.0" prefWidth="250.0" text="Bingo">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button fx:id="blackjack" layoutX="1000.0" layoutY="350.0" mnemonicParsing="false" onAction="#blackjack" prefHeight="100.0" prefWidth="250.0" text="Blackjack">
